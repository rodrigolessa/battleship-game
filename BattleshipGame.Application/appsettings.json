{
  "AllowedHosts": "*",
  "Serilog": {
    "LevelSwitches": {
      "controlSwitch": "Information"
    },
    "MinimumLevel": {
      "ControlledBy": "$controlSwitch",
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "ApmSettings": {},
  "HealthCheck": {
    "Name": "battleship-game-service",
    "Uri": "http://localhost:5000/health",
    "UriCheckTimeoutInSeconds": 3
  },
  "IdempotencySettings": {
    "IdempotencyKeyHeader": "idempotency-key",
    "IdempotencyKeyMaximumLength": 36,
    "RemoveDashesIfGuid": false
  },
  "CorsSettings": {
    "ClientsUrl": [
      "*"
    ]
  },
  "MessageBrokerSettings": {
    "HostName": "http://localhost/",
    "HostPort": "15672",
    "UserName": "xpto",
    "UserPassword": "xpto",
    "WaitForMessageToBeSent": false,
    "MaxLength": 12800000,
    "MessageMaxLength": 64000,
    "MessageTimeToLiveInMilliseconds": 60000,
    "MessageType": 0,
    "NonRetryableExceptionType": [
      "FluentValidation.ValidationException"
    ],
    "DefaultChannelName": "amqp.direct",
    "Channels": [
      {
        "Name": "amqp.direct",
        "Type": 1,
        "UsePersistentStorage": false,
        "AutoDelete": false,
        "PrefetchCount": 0,
        "RetryCount": 3,
        "RetryIntervalInSeconds": 60,
        "MessageWaitTimeoutInMilliseconds": 200,
        "CanReceiveMessages": true,
        "CanPublishMessages": true,
        "UseSessions": true,
        "QueueName": "xpto",
        "Key": "DefaultCommandsChannel"
      }
    ]
  },
  "ConnectionStrings": {
    "BattleshipGameEventsDb": "xpto",
    "BattleshipGameReadModelDb": "xpto"
  },
  "EventSourcingRepositorySettings": {
    "ConnectionString": "BattleshipGameEventsDb",
    "UseInMemory": false,
    "ApplyMigrations": false,
    "MigrationsTableName": "EventSourcingMigrations",
    "SchemaName": "eventstore",
    "UsePool": true,
    "PoolSize": 256,
    "IsQueryLogEnabled": false
  },
  "ReadModelRepositorySettings": {
    "ConnectionString": "BattleshipGameReadModelDb",
    "UseInMemory": false,
    "ApplyMigrations": false,
    "MigrationsTableName": "ReadModelMigrations",
    "SchemaName": "battleship",
    "UsePool": true,
    "PoolSize": 256,
    "IsQueryLogEnabled": false
  },
  "RedisSettings": {
    "EnableCache": true,
    "UseSsl": true,
    "Password": "",
    "AbortOnConnectFail": true,
    "HostList": [
      {
        "Address": "xpto",
        "HostList__0__Port": 6380
      }
    ],
    "Prefix": "battleship-game",
    "UseHashSets": true,
    "UseGZip": true,
    "ConnectTimeoutInMilliseconds": 1000,
    "SyncTimeoutInMilliseconds": 1000,
    "AsyncTimeoutInMilliseconds": 2000,
    "RetryCount": 1,
    "RetryIntervalInMilliseconds": 300,
    "PoolSize": 10
  },
  "HangfireSettings": {
    "EnableHangfire": true,
    "RedisConnectionString": "",
    "Prefix": "hangfire:battleship-game:",
    "Timeout": 3,
    "EnqueueJobRetryCount": 3,
    "DashboardPath": "/hangfire",
    "DashboardIsReadOnly": true,
    "ShutdownTimeoutInSeconds": 60
  },
  "Authentication": {
    "Keycloak": {}
  }
}